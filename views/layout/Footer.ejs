<% for(let error of flash.errors) {%>
  <div class="flash alert alert-danger fixed-top m-5 text-center w-25 mx-5">
    <h6><%= error %></h6>
  </div>
<% } %>

<% for(let msg of flash.success) {%>
  <div class="flash alert alert-success fixed-top m-5 text-center w-25 mx-5">
    <h6><%= msg %></h6>
  </div>
<% } %>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
<script src="/js/app.js"></script>
</body>
</html>